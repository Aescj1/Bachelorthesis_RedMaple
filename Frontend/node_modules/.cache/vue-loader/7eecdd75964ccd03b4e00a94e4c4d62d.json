-content>\r\n        </v-list-tile>\r\n\r\n      <!-- Defines the function groupd \"Actions\" !-->\r\n        <v-list-group\r\n          prepend-icon=\"remove_red_eye\"\r\n          no-action\r\n          v-if=\"this.$route.path == '/table'\"\r\n        >\r\n          <v-list-tile slot=\"activator\">\r\n            <v-list-tile-title>Actions</v-list-tile-title>\r\n          </v-list-tile>\r\n            <v-list-tile \r\n            v-for=\"item in headers\"\r\n            :key=\"item.value\"\r\n            @click=\"hideHeader(item)\" \r\n            v-model=\"item.show\"\r\n            >\r\n            <v-list-tile-action>\r\n              <v-switch\r\n                readonly\r\n                v-model=\"item.show\"\r\n                color= red\r\n                :label=\"item.text\"\r\n              ></v-switch>\r\n            </v-list-tile-action>\r\n            </v-list-tile>\r\n          </v-list-group>\r\n\r\n          <v-list-tile>\r\n          <v-list-tile-action>\r\n            <v-icon>group</v-icon>\r\n          </v-list-tile-action>\r\n          <v-list-tile-content>\r\n            <v-list-tile-title>User management</v-list-tile-title>\r\n          </v-list-tile-content>\r\n          </v-list-tile>\r\n\r\n        <v-list-tile @click=\"logout()\">\r\n          <v-list-tile-action>\r\n            <v-icon>settings</v-icon>\r\n          </v-list-tile-action>\r\n          <v-list-tile-content>\r\n            <v-list-tile-title>Logout</v-list-tile-title>\r\n          </v-list-tile-content>\r\n          </v-list-tile>\r\n\r\n      </v-list>\r\n  </v-card>\r\n</template>\r\n\r\n<script>\r\nimport {bus} from '../main.js'\r\n\r\n  export default {\r\n\r\n    data: ()=>({\r\n      /**This is the Information for the Toggle Buttons */\r\n      headers:[\r\n        {  index:0, text: 'Bact Nr-', show:true},\r\n        {  index:1, text: 'Alternative ID', show:false },\r\n        {  index:2, text: 'Priority', show:true},\r\n        {  index:3, text: 'Pathogen', show:true